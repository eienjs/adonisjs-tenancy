{
  "name": "@eienjs/adonisjs-tenancy",
  "description": "Automatic multi-tenancy for AdonisJS. No code changes needed",
  "version": "1.0.0",
  "type": "module",
  "main": "",
  "module": "",
  "types": "",
  "files": [],
  "exports": {},
  "scripts": {
    "prepare": "is-in-ci || husky",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "gen:docs": "typedoc --options typedoc.json",
    "changelog": "auto-changelog -p && git add CHANGELOG.md",
    "prebuild": "pnpm lint:check && pnpm typecheck",
    "build": "tshy",
    "postbuild": "pnpm run gen:docs && git add docs/*",
    "release": "np",
    "version": "pnpm run build && pnpm run changelog"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@japa/assert": "^3.0.0",
    "@japa/runner": "^3.1.4",
    "@jsprismarine/typedoc-material-theme": "^1.0.4",
    "@nodecfdi/eslint-config": "^2.2.3",
    "@nodecfdi/prettier-config": "^1.3.1",
    "@nodecfdi/tsconfig": "^1.7.2",
    "@swc/core": "^1.9.2",
    "@types/node": "^22.9.0",
    "auto-changelog": "^2.5.0",
    "c8": "^10.1.2",
    "copyfiles": "^2.4.1",
    "eslint": "^9.14.0",
    "husky": "^9.1.6",
    "is-in-ci": "^1.0.0",
    "np": "^10.0.7",
    "openapi-types": "^12.1.3",
    "prettier": "^3.3.3",
    "ts-node-maintained": "^10.9.4",
    "tshy": "^3.0.2",
    "typedoc": "^0.26.11",
    "typescript": "^5.6.3"
  },
  "overrides": {
    "eslint-plugin-import": "npm:@favware/skip-dependency@latest"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowAny": [
        "eslint",
        "@typescript-eslint/parser"
      ]
    }
  },
  "author": "Fernando Isidro <luffynando@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/eienjs/adonisjs-tenancy",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eienjs/adonisjs-tenancy.git"
  },
  "bugs": {
    "url": "https://github.com/eienjs/adonisjs-tenancy/issues"
  },
  "keywords": [
    "adonis",
    "adonisjs",
    "multi-tenancy",
    "tenancy",
    "multi-database",
    "lucid"
  ],
  "engines": {
    "node": ">=20.6.0"
  },
  "packageManager": "pnpm@9.12.3",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "prettier": "@nodecfdi/prettier-config/pkg",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "auto-changelog": {
    "template": "keepachangelog",
    "hideCredit": true
  },
  "np": {
    "message": "chore(release): :tada: %s",
    "tag": "latest",
    "branch": "main",
    "testScript": "test:run"
  },
  "tshy": {
    "selfLink": false,
    "dialects": [
      "esm"
    ],
    "exports": {
      ".": "./src/index.ts"
    }
  }
}
